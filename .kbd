! Switch windows key and caps lock.
remove Lock = Caps_Lock
remove mod4 = Super_L
remove mod2 = Control_L
remove mod1 = Alt_L
! keycode 107 = Super_L
! keycode 218 = Super_L
! keysym Print = Super_R
keysym Super_L = Caps_Lock
keysym Caps_Lock = Super_L
keysym Alt_L = Escape
add Lock = Caps_Lock
add mod4 = Super_L
